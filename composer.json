{
    "name": "marcel-mathias-nolte/contao-filesmanager-fileusage",
    "type": "contao-bundle",
    "description": "This bundles displays an icon, which shows whether an file is used anywhere or not and shows details on where it is used if you click on it.",
    "homepage": "https://github.com/marcel-mathias-nolte",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Leo Feyer",
            "homepage": "https://github.com/leofeyer"
        },
        {
            "name": "Contao Community",
            "homepage": "https://contao.org/contributors"
        },
        {
            "name": "Marcel Mathias Nolte",
            "homepage": "https://github.com/marcel-mathias-nolte"
        }
    ],
    "require": {
        "php": "^7.2 || 8.*",
        "contao/core-bundle": "^4.4",
        "contao-community-alliance/composer-plugin": "^2.4.0 || 3.*"
    },
    "extra": {
        "contao-manager-plugin": "MarcelMathiasNolte\\ContaoFilesmanagerFileusageBundle\\ContaoManager\\Plugin"
    },
    "autoload": {
        "psr-4": {
            "MarcelMathiasNolte\\ContaoFilesmanagerFileusageBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MarcelMathiasNolte\\ContaoFilesmanagerFileusageBundle\\Tests\\": "tests/"
        }
    },
    "support": {
        "issues": "https://github.com/marcel-mathias-nolte/contao-filesmanager-fileusage/issues",
        "source": "https://github.com/marcel-mathias-nolte/contao-filesmanager-fileusage"
    },
    "config": {
        "allow-plugins": {
            "contao-components/installer": true,
            "contao-community-alliance/composer-plugin": true
        }
    }
}
